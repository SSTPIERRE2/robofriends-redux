{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6",
      "DOM"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "esnext",
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext"
  },
  "typeRoots": [
    "./typings",
    "node_modules/@types"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "prettier.config.js",
    "generateAllModels.ts"
  ],
  "include": [
    "src"
  ]
}
